const removeClasses = ["ul", "p", "li", "h1", "h2", "h3", "h4", "h5", "h6", "span", "a", "div"]
for (const item in removeClasses) {
    console.log(removeClasses[item])
    try {
        document.querySelectorAll(removeClasses[item]).forEach((userItem) => {
            userItem.removeAttribute('class')
            userItem.removeAttribute('id')
            userItem.removeAttribute('style')
        });
    } catch (error) {}
}